    }
  },
  "license": "Apache-2.0 OR MPL-2.0",
  "devDependencies": {
    "@cc-voice-reporter/eslint-config": "*",
    "@types/node": "^25.2.3",
    "eslint": "^9.0.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "zod": "^4.3.6"
